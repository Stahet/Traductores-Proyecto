program {
	using
 		int i,j,k;
 		bool b,c;
 		set s;
 	in

	if(true) {
		using
			set i;
			bool b;
		in
		i = {1,2,3};
 		# nótese que al lado izquierdo del `min`, `i` es int
 		# y al lado derecho es set
	for i min 1 <-> (2 <+> i)  do {
        print i;          
	};
	} else {
		using bool i; in
	};
}
