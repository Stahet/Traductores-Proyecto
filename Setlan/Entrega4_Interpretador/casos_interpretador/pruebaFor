program {
    using
        set s, t;
    in
    s = {5,0,-10,60,-1};
    for i min s do {
        print i," ";
        s = s ++ {i * 2}; # unión de conjuntos
    };
    println "";
    print s;
    
    println "";println "";
    t = {-5,78,2,10,6};
    for i max t do {
        print i," ";
        t = t ++ {i + 2}; # unión de conjuntos
    };
    println "";
    print t;
}